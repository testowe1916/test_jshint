{
  "name": "nauka-git",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html && npm run test:js",
    "test:html": "glob-run nu-html-checker *.html",
    "test:js": "npm run jshint",
    "build": "npm run build:sass && npm run build:autoprefixer && npm run test",
    "build:sass": "sass --sourcemap=none --update sass:css",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "watch:sass": "sass --watch sass:css",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css",
    "watch:browsersync": "",
    "jshint": "jshint *.js"


  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testowe1916/testy-git.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/testowe1916/testy-git/issues"
  },
  "homepage": "https://github.com/testowe1916/testy-git#readme",
  "dependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.18.13",
    "glob-run": "^0.1.6",
    "jshint": "^2.9.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.2"
  }
}
